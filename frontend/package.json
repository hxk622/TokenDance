{
  "name": "tokendance-frontend",
  "version": "0.1.0",
  "description": "TokenDance - Universal AI Agent Platform Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "build:with-check": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "npm run type-check && npm run build",
    "precommit": "npm run type-check",
    "prepare": "cd .. && husky"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@types/d3": "^7.4.3",
    "@vueuse/core": "^10.7.1",
    "axios": "^1.6.5",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "highlight.js": "^11.11.1",
    "klinecharts": "10.0.0-beta1",
    "lucide-vue-next": "^0.562.0",
    "marked": "^17.0.1",
    "monaco-editor": "^0.55.1",
    "pinia": "^2.1.7",
    "recharts": "^3.6.0",
    "vue": "^3.4.15",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.1",
    "vue-tsc": "^3.2.2"
  }
}
